        <calculation><![CDATA[function getAge(birthdate) {
  var now = new GlideDateTime();
  var birthdateDateTime = new GlideDateTime(birthdate);
  
  var diff = gs.getDateDiff(birthdateDateTime.getNumericValue(), now.getNumericValue(), 'days');
  
  var years = Math.floor(diff / 365);
  var months = Math.floor((diff % 365) / 30);
  var days = (diff % 365) % 30;
  
  return {
    years: years,
    months: months,
    days: days
  };
}

var birthdate = '1937-08-23 00:00:00';
var age = getAge(birthdate);
gs.info('Age: ' + age.years + ' years, ' + age.months + ' months, ' + age.days + ' days');]]></calculation>
